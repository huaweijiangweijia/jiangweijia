  <select id="getNewModuleId" parameterClass="java.lang.String" resultClass="java.lang.String">
    select 
	case when  count(*) = 0 then 
	concat(#value#,'001')
	else  
	LPAD(cast(max(t.id) + 1 as char),length(#value#)+3,'0' )
	end as id
	from t_modules_def t 
	where t.id like concat(#value#,'%') and t.id != #value# and length(t.id) = length(#value#)+3
</select>
<select id="getTheUserModules" parameterClass="map" resultMap="ibatorgenerated_BaseResultMap">
	select distinct md.* from 
			t_user_infor u,
			t_users_roles ur,
			t_roles_modules tm,
			t_modules_def md
	where u.id = ur.id
	and ur.role_id = tm.id
	and tm.module_id = md.id
	and u.id = #userId#
	and md.parent_id = #parentId#
	order by serial_number,id
</select>
  
