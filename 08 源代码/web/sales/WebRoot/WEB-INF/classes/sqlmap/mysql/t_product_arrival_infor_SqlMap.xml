  <!-- 储备入库能否作废，若返回值大于0 不允许作废 -->
  <select id="isCanInvaStockArr" parameterClass="String" resultClass="Integer">
    <![CDATA[
      SELECT COUNT(*) FROM t_product_arrival_detail ad
      , t_reserve_infor ri 
      WHERE ad.product_arrival_infor_id = #value# 
      AND ad.tools_id=ri.tools_id AND ri.amount-ad.arrival_amount < 0
    ]]>
  </select>
  
  <!-- 入库单是否允许作废 如果返回结果大于0 不允许作废 -->
  <select id="isCanInvalidArr" parameterClass="String" resultClass="Integer">
    <![CDATA[
      SELECT COUNT(*) FROM (
      SELECT ii.*,IFNULL(iii.delivery_amount,0) delivery_amount FROM ( 
      SELECT i.*,io.out_amount FROM (
      SELECT arrival_amount,od.contract_product_detail_id FROM t_product_arrival_detail ad,t_order_detail od
      WHERE ad.order_detail_id = od.id
      AND ad.product_arrival_infor_id = #value#
      ) i LEFT JOIN 
      (
      SELECT SUM(osd.amount) out_amount,osd.contract_product_detail_id 
      FROM t_out_stock_detail osd,t_out_stock_infor osi 
      WHERE  osd.out_stock_infor_id = osi.id
      AND osi.status <> 2
      GROUP BY osd.contract_product_detail_id 
      ) io ON i.contract_product_detail_id = io.contract_product_detail_id
      ) ii LEFT JOIN 
      (
      SELECT dd.contract_product_detail_id,SUM(dd.amount) delivery_amount 
      FROM t_delivery_infor di,t_delivery_detail dd 
      WHERE di.id = dd.delivery_infor_id AND di.status <> 4
      GROUP BY dd.contract_product_detail_id 
      ) iii
      ON ii.contract_product_detail_id = iii.contract_product_detail_id
      ) re WHERE re.arrival_amount > re.out_amount - re.delivery_amount
    ]]>
  </select>
  
