  <update id="updateMatAmountByInvoiceId" parameterClass="com.tl.resource.dao.pojo.TAccountsInfor">
    UPDATE t_accounts_infor y
	SET y.amount = amount +  #amount:DECIMAL#
	WHERE y.invoice_id = #invoiceId:VARCHAR# 
	AND y.account_type = 5 
	AND (
	SELECT 
	ifnull(SUM(
	CASE WHEN  t.account_type = 4 then
	t.amount
	WHEN t.account_type = 5 then
	-t.amount
	ELSE
	0
	END
	),0) FROM (SELECT tb.account_type,tb.amount,tb.product_code FROM t_accounts_infor tb) t WHERE t.product_code = #productCode:VARCHAR#) - #amount:DECIMAL# >= 0
  </update>
    <update id="selectMatOutAmountByProductCode" parameterClass="java.lang.String">
	SELECT 
	ifnull(SUM(
	CASE WHEN  account_type = 4
	amount
	CASE WHEN account_type = 5
	-amount
	ELSE
	0
	END
	),0) FROM t_accounts_infor WHERE product_code = #value:VARCHAR#
  </update>

