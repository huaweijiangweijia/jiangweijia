<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<action-mappings>
		<action path="/reserveOrder/listAction" scope="request" validate="false"></action>
		<action path="/reserveOrder/orderDetailsList" scope="request" validate="false"></action>
		<action path="/reserveOrder/orderDetail" scope="request" validate="false"></action>
		<action path="/reserveOrder/suppliersInforList" scope="request" validate="false"></action>
		<action path="/reserveOrder/addOrder" scope="request" validate="false"></action>
		<action path="/reserveOrder/updateOrder" scope="request" validate="false"></action>
		<action path="/reserveOrder/deleteOrder" scope="request" validate="false"></action>
		<action path="/reserveOrder/deleteOrderDetail" scope="request" validate="false"></action>
		<action path="/reserveOrder/productList" scope="request" validate="false"></action>
		<action path="/reserveOrder/submitAudit" scope="request" validate="false"></action>
		<action path="/reserveOrder/placeOrder" scope="request" validate="false"></action>
		<action path="/reserveOrder/currencyList" scope="request" validate="false"></action>
		<action path="/reserveOrder/orderExcel" scope="request" validate="false"></action>
	</action-mappings>

	<controller>
		<set-property property="processorClass"
			value="org.springframework.web.struts.DelegatingRequestProcessor" />
	</controller>
		
</struts-config>
