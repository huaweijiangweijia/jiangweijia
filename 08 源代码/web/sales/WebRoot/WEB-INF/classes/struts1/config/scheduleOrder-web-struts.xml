<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<action-mappings>
		<action path="/scheduleOrder/orderListAction" scope="request" validate="false"></action>
		<action path="/scheduleOrder/quotationList" scope="request" validate="false"></action>
		<action path="/scheduleOrder/quotationDetailList" scope="request" validate="false"></action>
		<action path="/scheduleOrder/supplierList" scope="request" validate="false"></action>
		<action path="/scheduleOrder/product" scope="request" validate="false"></action>
		<action path="/scheduleOrder/productList" scope="request" validate="false"></action>
		<action path="/scheduleOrder/OrderDetail" scope="request" validate="false"></action>
		<action path="/scheduleOrder/OrderDetailList" scope="request" validate="false"></action>
		<action path="/scheduleOrder/submitAudit" scope="request" validate="false"></action>
		<action path="/scheduleOrder/getSPSIList" scope="request" validate="false"></action>
		<action path="/scheduleSelfOrder/submitAudit" scope="request" validate="false"></action>
	</action-mappings>

	<controller>
		<set-property property="processorClass"
			value="org.springframework.web.struts.DelegatingRequestProcessor" />
	</controller>
		
</struts-config>
