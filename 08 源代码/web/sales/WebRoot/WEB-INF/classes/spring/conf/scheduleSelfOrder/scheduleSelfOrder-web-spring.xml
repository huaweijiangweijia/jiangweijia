<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
   http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
   http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd"
	default-autowire="byType">
	
	<bean name="/scheduleSelfOrder/quotationDetailList" class="com.tl.resource.web.scheduleSelfOrder.QuotationDetailListAction">
		<property name="scheduleSelfOrderService">
			<ref bean="ScheduleSelfOrderServiceImpl"/>
		</property>
	</bean>
	
	
	<bean name="/scheduleSelfOrder/product" class="com.tl.resource.web.scheduleSelfOrder.QuotationDetailListBySupplierAction">
		<property name="scheduleSelfOrderService">
			<ref bean="ScheduleSelfOrderServiceImpl"/>
		</property>
	</bean>
	
	<bean name="/scheduleSelfOrder/productList" class="com.tl.resource.web.scheduleSelfOrder.productListAction">
		<property name="scheduleSelfOrderService">
			<ref bean="ScheduleSelfOrderServiceImpl"/>
		</property>
	</bean>
	
	<bean name="/scheduleSelfOrder/orderDetailList" class="com.tl.resource.web.scheduleSelfOrder.OrderDetailListAction">
		<property name="scheduleSelfOrderService">
			<ref bean="ScheduleSelfOrderServiceImpl"/>
		</property>
	</bean>
	
	<bean name="/scheduleSelfOrder/orderDetail" class="com.tl.resource.web.scheduleSelfOrder.OrderDetailAction">
		<property name="scheduleSelfOrderService">
			<ref bean="ScheduleSelfOrderServiceImpl"/>
		</property>
	</bean>
	
	<bean name="/scheduleSelfOrder/submitAudit" class="com.tl.resource.web.scheduleSelfOrder.submitScheduleSelfOrderAction">
		<property name="auditService">
			<ref bean="auditService"/>
		</property>
	</bean>
</beans>