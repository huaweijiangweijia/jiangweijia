  <!-- 得到需要修改的库存 -->
  <select id="getCanInvaArrDetail" parameterClass="String"  resultClass="List" resultMap="invalidResultMap">
    <![CDATA[
        SELECT ad.id , ri.id AS tools_id, ad.arrival_amount, ri.amount as actual_amount 
        FROM t_product_arrival_detail ad, t_reserve_infor ri 
    	WHERE ad.product_arrival_infor_id = #value# 
    	AND ad.tools_id=ri.tools_id AND ri.amount-ad.arrival_amount >= 0
    ]]>
  </select>
  
