  <select id="getArrivalDetailByArrId" parameterClass="String" resultClass="List" resultMap="ibatorgenerated_BaseResultMap">
    <![CDATA[
      select * from t_product_arrival_detail t where t.product_arrival_infor_id=#value#
    ]]>
  </select>
  
