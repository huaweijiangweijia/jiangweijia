<select id="getAuditorsInsCount" resultClass="java.lang.Integer" parameterClass="map" >
	SELECT count(*)
	FROM t_audit_instance ins2,(
		SELECT MIN(audit_order) audit_order,business_id,flow_infor_id
		FROM t_audit_instance ins     
		WHERE flow_infor_id = #flow_infor_id#  
		GROUP BY business_id,flow_infor_id
	) aa WHERE ins2.business_id = aa.business_id AND ins2.audit_order = aa.audit_order
	AND ins2.flow_infor_id =  aa.flow_infor_id
	AND ins2.audit_person_id = #audit_person_id#
</select>
  
<select id="getAuditorsIns" resultMap="auditInstanceDtoresult" parameterClass="map" >
	SELECT bb.*,cc.user_id,cc.user_name FROM (
		SELECT ins2.*,(SELECT IFNULL(MAX(re.batch_number),0) + 1 FROM t_audit_batch_record re 
			WHERE re.business_id = ins2.business_id) audit_batch     
			FROM t_audit_instance ins2,(
				SELECT MIN(audit_order) audit_order,business_id,flow_infor_id
				FROM t_audit_instance ins     
				WHERE flow_infor_id = #flow_infor_id#  
				GROUP BY business_id,flow_infor_id
			) aa WHERE ins2.business_id = aa.business_id AND ins2.audit_order = aa.audit_order
			AND ins2.flow_infor_id =  aa.flow_infor_id
			AND ins2.audit_person_id = #audit_person_id#
			ORDER BY id DESC     
			LIMIT $startIndex$,$pageSize$
		) bb LEFT JOIN (
		SELECT wtd.* FROM t_work_trust_record wtr,t_work_trust_detail wtd
		WHERE wtr.id = wtd.trust_record_id
		AND wtr.op_type = 0
		AND wtd.tabel_name = 't_audit_instance'
		) cc
		ON bb.id = cc.table_id
</select>
  
  
