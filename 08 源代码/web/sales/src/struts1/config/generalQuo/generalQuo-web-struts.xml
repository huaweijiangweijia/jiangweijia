<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<action-mappings>
		<action path="/generalQuo/listAction" scope="request" validate="false"></action>
		<action path="/generalQuo/baseListAction" scope="request" validate="false" parameter="m"></action>
		<action path="/generalQuo/getCustomerAction" scope="request" validate="false"></action>
		<action path="/generalQuo/addGeneralQuoAction" scope="request" validate="false"></action>
		<action path="/generalQuo/getCompanyAction" scope="request" validate="false"></action>
		<action path="/generalQuo/deleteQuoAction" scope="request" validate="false"></action>
		<action path="/generalQuo/getQuoDetailAction" scope="request" validate="false"></action>
		<action path="/generalQuo/updateQuoAction" scope="request" validate="false"></action>
		<action path="/generalQuo/getQuoAction" scope="request" validate="false"></action>
		<action path="/generalQuo/deleteQuoProAction" scope="request" validate="false"></action>
		<action path="/generalQuo/submitAuditAction" scope="request" validate="false"></action>
		<action path="/generalQuo/getHistoryPriAction" scope="request" validate="false"></action>
		<action path="/generalQuo/getCusByCuscodeAction" scope="request" validate="false"></action>
		<action path="/generalQuo/getCurrencyAction" scope="request" validate="false"></action>
		<action path="/generalQuo/submitContractAction" scope="request" validate="false"></action>
		<action path="/generalQuo/excelAction" scope="request" validate="false"></action>
		
		<action path="/generalQuo/submitOrderExpectedAction" scope="request" validate="false" parameter="method"></action>
	</action-mappings>

	<controller>
		<set-property property="processorClass"
			value="org.springframework.web.struts.DelegatingRequestProcessor" />
	</controller>
</struts-config>
